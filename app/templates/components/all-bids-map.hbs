<div class="container">
  {{#each bids as |bid|}}
    {{bid.title}}
  {{/each}}
  {{#each placedMarkers as |marker|}}
    {{marker.lat}}
  {{/each}}
  {{#if refresh}}
    {{g-maps name="bidmap" click='placeMarker' lat=lat lng=lng zoom=zoom markers=markers}}
  {{/if}}
  <ul>
    <li>clickLat: {{clickLat}}</li>
    <li>clickLng: {{clickLng}}</li>
    <li>placed markers:
      {{#each markers as |marker|}}
        {{marker.lat}}
      {{/each}}
    </li>
  </ul>
  {{md-btn action='showMap' text='show bids'}}
  {{md-btn action='testFunction' text='test'}}
  {{log bids.length}}
  <a href="#" {{action 'testFunction' bids}}>cornholio</a>
</div>
